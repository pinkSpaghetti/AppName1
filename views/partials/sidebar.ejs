<div class="col-1 back-panel">
    <div class="channel-links">
        <form action="/account" method="GET">
            <div class="home-link">
                <button id="profile-update">
                    <i class="fas fa-user"></i>
                </button>
            </div>
        </form>
        <hr>
        <form action="/channels" method="GET" id="channels">
            <div class="new-channel">
                <button type="submit" form="channels" id="add-channels">
                    <i class="fas fa-plus"></i>
                </button>
            </div>
        </form>
    </div>
    <form action="/logout" method="GET" id="logout">
        <hr>
        <div class="logout">
            <button type="submit" form="logout" id="logout-button">
                <i class="fas fa-sign-out-alt"></i>
            </button>
        </div>
    </form>
</div>